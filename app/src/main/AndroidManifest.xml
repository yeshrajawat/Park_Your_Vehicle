<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codingblocks.parkyourvehicle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkYourVehicle"
        android:usesCleartextTraffic="true">
        <activity android:name=".User_Requests"
            android:parentActivityName=".MyDashBoard"></activity>
        <activity
            android:name=".ProfileUI"
            android:parentActivityName=".MyDashBoard" />
        <activity
            android:name=".vehicle_list"
            android:parentActivityName=".MyDashBoard" />
        <activity android:name=".MyDashBoard" />
        <activity android:name=".Dash" />
        <activity
            android:name=".Empl"
            android:parentActivityName=".MyDashBoard" />
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".Main_display"
            android:label="@string/title_activity_main_display"
            android:theme="@style/Theme.ParkYourVehicle.NoActionBar" />
        <activity
            android:name=".add_vehicle"
            android:parentActivityName=".MyDashBoard" />
        <activity
            android:name=".Dashboard"
            android:parentActivityName=".Dash" />
        <activity
            android:name=".SignIn"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.MaterialComponents.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login_Page"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
    </application>

</manifest>